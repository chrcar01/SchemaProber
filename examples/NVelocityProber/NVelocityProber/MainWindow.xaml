<Window x:Class="NVelocityProber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NVelocity Schema Prober Examples" Height="350" Width="525">
    <Grid>
		<StackPanel
			VerticalAlignment="Center"
			Orientation="Vertical">
			<Button
				Margin="20"
				Content="Class Generator"
				Height="50"
				Width="150"
				x:Name="ClassGeneratorButton"
				Click="ClassGeneratorButton_Click" />
			<Button
				Margin="20"
				Content="Repo Generator"
				Height="50"
				Width="150"
				x:Name="RepoGeneratorButton"
				Click="RepoGeneratorButton_Click" />
		</StackPanel>
	</Grid>
</Window>
